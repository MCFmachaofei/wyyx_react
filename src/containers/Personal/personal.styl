@import "../../assets/stylus/mixins.styl"
$rem = 750/10rem
.personal_wrap
  margin-top (76/$rem)
  height (100%)
  background #f7f7f7
  box-sizing border-box
  padding 0 (20/$rem)
  position relative
  .titile
    width 100%
    height (60/$rem)
    font-size (36/$rem)
    p
      margin-top (10/$rem)
  .out
    position absolute
    right 0
    left 0
    bottom (20%)
    margin auto
    width (80%)
    height (60/$rem)
    box-sizing border-box
    font-size (36/$rem)
    line-height (60/$rem)
    text-align center
    color white
    background-color #b4282d
    border-radius (10/$rem)


